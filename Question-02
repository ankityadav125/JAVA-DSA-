//    Print the messege along with values and sum
//    Ex - The sum of 45 & 12  = 57

package Java_Questions;

public class Question_02 {
    public static void main(String[] args) {
        int num1=45;
        int num2=12;
        int sum=num1+num2;
        System.out.println("The sum of " +num1+ " & " +num2+ "= "+sum);
    }
}
